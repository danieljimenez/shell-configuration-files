# VCS repos	
Host github.com
	User git
	ForwardAgent no
Host gist.github.com
	User git
	ForwardAgent no
Host bitbucket.org
	User git
	ForwardAgent no
Host git-codecommit.*.amazonaws.com
	User APKAI25OV6N2BJOWKWXQ
	ForwardAgent no

# Misc Servers
Host devops
	User fore
Host build-agent-*
	User foreflight
	ForwardAgent yes

Host i-* *.amazonaws.com
  User ec2-user

# Default aka *
Host *
	User ec2-user
	StrictHostKeyChecking no
	RemoteForward 127.0.0.1:52698 127.0.0.1:52698
	ForwardAgent yes
	LogLevel ERROR
	# UserKnownHostsFile /dev/null
	KexAlgorithms diffie-hellman-group1-sha1,diffie-hellman-group14-sha1,diffie-hellman-group-exchange-sha1,diffie-hellman-group-exchange-sha256,ecdh-sha2-nistp256,ecdh-sha2-nistp384,ecdh-sha2-nistp521

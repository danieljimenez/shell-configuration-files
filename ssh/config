# VCS repos	
Host github.com
	User git
	ForwardAgent no
	UseRoaming no
Host gist.github.com
	User git
	ForwardAgent no
	UseRoaming no
Host bitbucket.org
	User git
	ForwardAgent no
	UseRoaming no

# Misc Servers
Host devops
	User fore

Host i-* *.amazonaws.com
  User ec2-user

# Default aka *
Host *
  User ec2-user
	StrictHostKeyChecking no
	RemoteForward 127.0.0.1:52698 127.0.0.1:52698
	ForwardAgent yes
	LogLevel ERROR
	UseRoaming no
	UserKnownHostsFile /dev/null

